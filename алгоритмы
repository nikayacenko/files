#include <iostream>
#include <vector>
#include <fstream>
using namespace std;
int select(vector<int>& v) {
	int n = v.size();
	int op = 0;
	int mn = 0, temp=0;
	for (int i = 0; i < n-1; i++) {
		mn = i;
		op++;
		for (int j = i + 1; j < n; j++) {
			if (v[j] < v[mn]) { 
				mn = j; 
				op += 1;
			}
			if (i != mn) {
				temp = v[i];
				v[i] = v[mn];
				v[mn] = temp;
				op += 3;
			}
			op+=2;
		}
	}
	return op;
}
int main() {
	int res[10];
	ofstream fout("file.txt");
	for (int n = 100, k=0; n <= 1000; n += 100, k++) {
		vector<int> mas;
		mas.resize(n);
		for (int i = 0, j=n; i < n; i++, j--){
			//mas[i] = rand() % 100;
			mas[i] = j;
		}
		//res[k] = select(mas);
		fout << n << " " << select(mas) << endl;
		
	}
	/*for (int i = 0; i < 10; i++)
		fout << res[i] << endl;*/
}
